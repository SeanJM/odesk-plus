{
  "name": "oDesk Plus",
  "version": "0.7.2.1",
  "manifest_version": 2,
  "description": "Enhance the usability of oDesk. See average rate paid by the provider, highest rate paid, timezone and country.",
  "icons": {
    "16": "/images/plugin-icon/odesk-icon_16x16.png",
    "48": "/images/plugin-icon/odesk-icon_48x48.png",
    "128": "/images/plugin-icon/odesk-icon_128x128.png"
    },
  "permissions": [
  "management",
  "http://*/*",
  "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.odesk.com/*"],
      "js": ["js/jquery.js","js/loader.js"]
    }
  ],
  "web_accessible_resources": [
    "images/sprites_icons.png",
    "images/sprites_ui.png",
    "css/styles.css",
    "js/db.js",
    "js/init.js",
    "js/header.js",
    "js/isPage.js",
    "js/job_search.js",
    "js/job_apply.js",
    "js/template.js",
    "templates/jobApply.html",
    "templates/templates.html"
  ]
}